<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="ui.freecrm test">
    <parameter name="useCloudEnv" value="false"></parameter>
    <parameter name="envName" value="browserstack"></parameter>
    <parameter name="os" value="windows"></parameter>
    <parameter name="osVersion" value="10"></parameter>
    <parameter name="browserVersion" value="100"></parameter>
    <parameter name="browserName" value="Chrome"></parameter>
    <parameter name="url" value="https://ui.freecrm.com/"></parameter>
    <test name="ui.freecrm tests">
        <classes>
                <class name="test.LoginTest"></class>
                 <class name="test.SearchTest"></class>
            <class name="test.DropDownTest"></class>
            <class name="test.MouseHoverTest"></class>
            <class name="test.LogoTest"></class>
            <class name="test.CreateTest"></class>
            <class name="test.LogOutTest"></class>
            <class name="test.PopUpTest"></class>
            <class name="test.CalendarTest"></class>
            <class name="test.RadioTest"></class>

        </classes>
    </test>
</suite>